package labexercises8;
import java.util.Scanner;
public class LabExercises8 
{
    public static void main(String[] args) 
    {
        Scanner in = new Scanner(System.in);
        number1
        int num, sum;

        num = in.nextInt();

        if (num < 0)
            num = -num;
        sum = 0;       
        while (num > 0) 
        {
            sum = sum + num % 10;           
            num = num / 10;                
        }
        System.out.println(sum);
    }
}
